
@{
    ViewBag.Title = "student_panel";
}

@Html.Raw(TempData["LoginSuccess"])

<link href="~/Temp/assets2/css/css.css" rel="stylesheet" />
<link href="~/Temp/assets2/css/bootstrap.css" rel="stylesheet" />
<link href="~/Temp/assets2/css/custom.css" rel="stylesheet" />
<link href="~/Temp/assets2/css/font-awesome.css" rel="stylesheet" />

<body>
    <div id="wrapper">
        <nav class="navbar navbar-default navbar-cls-top " role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="index.html">@Session["studentname"]</a>
            </div>
            <div style="color: white;padding: 15px 50px 5px 50px;float: right;font-size: 16px;">@Session["Name"] &nbsp;<a href="#" class="btn btn-danger square-btn-adjust">Logout</a></div>
        </nav>
        <!-- /. NAV TOP  -->
        <nav class="navbar-default navbar-side" role="navigation">
            <div class="sidebar-collapse">
                <ul class="nav" id="main-menu">
                    <li class="text-center">
                        <img src="~/Temp/assets/img/find_user.png" class="user-image img-responsive" />
                    </li>
                    <li>
                        <a class="active-menu" href="@Url.Action("student-pannel")"><i class="fa fa-dashboard fa-3x"></i> Dashboard</a>
                    </li>
                    <li>
                        <a href=""><i class="fa fa-users fa-3x"></i>Exibition<span class="fa arrow"></span></a>
                        <ul class="nav nav-second-level">
                            <li>
                                <a href="@Url.Action("student_Exibition","Student")">View</a>
                            </li>
                
                        </ul>
                    </li>
                 
                </ul>   

            </div>

        </nav>
        <!-- /. NAV SIDE  -->
        <div id="page-wrapper">
            <div id="page-inner">
                <div class="row">
                    <div class="col-md-12">
                        <h2>Student Dashboard</h2>
                        <h5>Welcome @Session["studentname"] , Love to see you back. </h5>
                        <div class="Insert_main" id="staff_inser">
                            <section class="sec_insert">
                                <div class="view_comp">
                                    <div class="card text-white mb-3" style="max-width: 18rem; border:1px solid #808080; float:right;padding:10px 12px">
                                        <div class="card-header">Compitition</div>
                                        <div class="card-body">
                                            <h5 class="card-title">Check it</h5>
                                            @*@Html.ActionLink("Click", "Compitition_view_for_student", "Staff",new { @class= "btn btn-danger" })*@
                                            <a href="@Url.Action("Compitition_view_for_student","Staff")" class="btn btn-warning">Click</a>
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /. ROW  -->
    </div>
</body>


<script src="~/Temp/assets2/js/bootstrap.min.js"></script>
<script src="~/Temp/assets2/js/custom.js"></script>
<script src="~/Temp/assets2/js/jquery-1.10.2.js"></script>
<script src="~/Temp/assets2/js/jquery.metisMenu.js"></script>







